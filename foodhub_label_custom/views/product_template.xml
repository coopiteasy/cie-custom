<?xml version="1.0" ?>
<odoo>
    <record id="foodhub_product_form" model="ir.ui.view">
        <field name="name">foodhub.product.template.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="beesdoo_product_label.product_view_form" />
        <field name="arch" type="xml">
            <field name="main_seller_id" position="after">
                <field name="producer" />
                <field name="location" />
                <field name="distance" />
            </field>
        </field>
    </record>
</odoo>
