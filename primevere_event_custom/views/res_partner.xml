<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="view_partner_primevere_form" model="ir.ui.view">
        <field name="name">res.partner.primevere.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="arch" type="xml">
            <xpath expr="//group" position="after">
                <group name="primevere">
                    <group>
                        <field name="nb_poster" />
                        <field name="nb_flyer" />
                        <field name="journal" />
                        <field name="ticket_code" />
                    </group>
                </group>
            </xpath>
        </field>
    </record>

</odoo>
