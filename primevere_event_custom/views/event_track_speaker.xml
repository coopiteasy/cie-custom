<?xml version="1.0"?>
<odoo>
    <record model="ir.ui.view" id="view_event_track_speaker_form">
        <field name="name">event.track.speaker.form</field>
        <field name="model">event.track.speaker</field>
        <field
            name="inherit_id"
            ref="event_track_multi_speaker.view_event_track_speaker_form"
        />
        <field name="arch" type="xml">
            <field name="image" position="after">
                <field name="curriculum_vitae" />
                <field name="invitation_number" />
                <field name="invitation_code" />
                <field name="need_ticket" />
                <field name="need_reserved_car_place" />
                <field name="slot_wishes" />
            </field>
        </field>
    </record>

</odoo>
